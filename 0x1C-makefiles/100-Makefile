CC = gcc
SRC = main.c holberton.c
NAME = holberton
OBJ = $(SRC:.c=.o)
CFLAGS = -Wall -Werror -Wextra -pedantic
.PHONY: all executable clean oclean fclean re

all: executable

executable: m.h $(OBJ)
	$(CC) $(CFLAGS) $(OBJ) -o $(NAME)

clean:
	$(RM) *~ $(NAME)

oclean: 
	$(RM) $(OBJ)

fclean: clean oclean

re: oclean executable
