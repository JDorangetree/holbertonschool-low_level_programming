CC = gcc
SRC = main.c holberton.c
NAME = holberton
OBJ = $(SRC:.c=.o)
CFLAGS = -Wall -Werror -Wextra -pedantic

.PHONY: executable clean oclean fclean re

all: executable m.h

executable: $(OBJ)
	$(CC) $(CFLAGS) $(OBJ) -o $(NAME)

clean:
	$(RM) *~ $(NAME)

oclean: 
	$(RM) $(OBJ)

fclean: clean oclean

re: oclean executable
